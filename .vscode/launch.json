{
  "configurations": [
    {
      "name": "Cortex-Debug with OpenOCD",
      "cwd": "${workspaceRoot}",
      "executable": "./output/main.elf",
      "request": "launch",
      "type": "cortex-debug",
      "servertype": "openocd",
      "configFiles": [
        "interface/stlink.cfg",
        "target/stm32f3x.cfg"
      ],
      "searchDir": [],
      "runToEntryPoint": "main",
      // "showDevDebugOutput": "raw",
      "liveWatch": {
        "enabled": true,
        "samplesPerSecond": 15,
      },
      "swoConfig": {
        "enabled": false,
        "source": "probe",
        "cpuFrequency": 72000000,
        "swoFrequency": 2000000,
        "decoders": [
          {
            "port": 0,
            "type": "console",
            "label": "SWO output",
            "encoding": "ascii"
          },
          {
            "port": 1,
            "type": "graph",
            "encoding": "float",
            "graphId": "1",
            "scale": 1
          },
          {
            "port": 2,
            "type": "graph",
            "encoding": "float",
            "graphId": "2",
            "scale": 1
          },
        ]
      },
      "graphConfig": [
        {
          "label": "Graph 1",
          "timespan": 5,
          "type": "realtime",
          "annotate": false,
          "maximum": 256,
          "minimum": 0,
          "plots": [
            {
              "graphId": "1",
              "label": "data 1",
              "color": "#53753c"
            },
            {
              "graphId": "2",
              "label": "data 2",
              "color": "#955f20"
            }
          ]
        },
      ]
    }
  ],
  "version": "0.2.0"
}